{
  "name": "@haste-health/generated-ops",
  "version": "0.12.3",
  "homepage": "https://haste.health",
  "repository": {
    "type": "git",
    "url": "https://github.com/HasteHealth/HasteHealth.git"
  },
  "main": "./lib/index.js",
  "type": "module",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "pnpm node --experimental-vm-modules $(pnpm bin jest)",
    "generate": "pnpm node ../cli/lib/hastehealth.js generate operations --output=./src/r4 && pnpm prettier --write ./src/r4/**/*.ts",
    "publish": "pnpm build && pnpm npm publish --access public --tolerate-republish"
  },
  "dependencies": {
    "@haste-health/operation-execution": "workspace:^"
  },
  "exports": {
    "./lib/r4/ops": "./lib/r4/ops.js",
    "./r4": "./lib/r4/ops.js"
  },
  "devDependencies": {
    "@haste-health/artifacts": "workspace:^",
    "@haste-health/fhir-types": "workspace:^",
    "@haste-health/haste-health.fhir.r4.core": "workspace:^",
    "@haste-health/hl7.fhir.r4.core": "workspace:^",
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "typescript": "5.9.2"
  },
  "files": [
    "README.md",
    "lib/**"
  ]
}